<div class="row form">
  <div class="col-md-4 col-md-offset-4">
    <%= render partial: "custom_sources/#{@type}/form", locals: {custom_source: @custom_source} %>
    <%= link_to "Delete", destroy_custom_source_path(@type, @custom_source), class: "btn btn-danger", method: :delete, data: {confirm: "Are you sure to delete this custom source ?"} %>
  </div>
</div>